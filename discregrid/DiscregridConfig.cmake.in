@PACKAGE_INIT@

list(FIND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}" HAS_DISCRETEGRID_CMAKE_MODULE_PATH)

if(HAS_DISCRETEGRID_CMAKE_MODULE_PATH EQUAL -1)
    list(APPEND CMAKE_MODULE_PATH ${CMAKE_CURRENT_LIST_DIR})
endif()

find_package(Eigen3 REQUIRED)
find_package(OpenMP REQUIRED)

if( NOT TARGET Discregrid)
    include("${CMAKE_CURRENT_LIST_DIR}/DiscregridTargets.cmake")
endif()

check_required_components(discregrid)

